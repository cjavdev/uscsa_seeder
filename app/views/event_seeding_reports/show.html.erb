<div class="col-md-8 col-xs-8">
  <h1><%= @report.event.name %> - Lineup</h1>

  <table class="table table-hover">
    <thead>
      <tr>
        <th> # </th>
        <th> First Name </th>
        <th> Last Name </th>
        <th> School </th>
        <th> Seeded </th>
      <tr>
    </thead>
    <% @report.heats.each_with_index do |heat, i| %>
      <thead>
        <tr>
          <th colspan="5"> Heat <%= i %> </th>
        <tr>
      </thead>
      <tbody>
      <% heat.each do |athlete| %>
        <tr>
          <td> <%= athlete.id %> </td>
          <td> <%= athlete.first_name %> </td>
          <td> <%= athlete.last_name %> </td>
          <td> <%= athlete.school_name %> </td>
          <td> <%= athlete.seeded %> </td>
        </tr>
      <% end %>
      </tbody>
    <% end %>
  </table>
</div>
