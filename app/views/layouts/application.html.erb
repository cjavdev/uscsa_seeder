<!DOCTYPE html>
<html>
<head>
  <title>UscsaSeeder</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container-fluid">
    <div class="row">
      <% if user_signed_in? %>
        <%= render 'layouts/sidebar' %>
      <% end %>

      <%= yield %>
    </div>
  </div>
</body>
</html>
